{
  "name": "Veneer",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Description ...",
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["scripts/content.js"]
    }
  ],
  "background": {
    "scripts": ["scripts/background.js"],
    "persistent": false
  },
  "devtools_page": "devtools.html",
  "browser_action": {
    "default_icon": {
      "19": "icons/19x19.png",
      "38": "icons/38x38.png"
    },
    "default_title": "vTerminal",
    "default_popup": "popup.html"
  }
}